{
    "tables": {
        "proteins": {
            "table": "protein_transformations_data",
            "joins": "inner join protein_transformations on protein_transformations.protein_transformation_id = protein_transformations_data.protein_transformation_id inner join proteins as before_proteins on before_proteins.protein_id = protein_transformations.before_protein_id inner join proteins as after_proteins on after_proteins.protein_id = protein_transformations.after_protein_id inner join sequences on sequences.sequence_id=before_proteins.sequence_id"
        },
        "domains": {
            "table": "domain_transformations_data",
            "joins": "inner join domain_transformations on domain_transformations.domain_transformation_id = domain_transformations_data.domain_transformation_id inner join domains as before_domains on before_domains.domain_id = domain_transformations.before_domain_id inner join domains as after_domains on after_domains.domain_id = domain_transformations.after_domain_id inner join proteins as before_proteins on before_proteins.protein_id = before_domains.protein_id inner join proteins as after_proteins on after_proteins.protein_id = after_domains.protein_id inner join sequences on sequences.sequence_id = before_proteins.sequence_id"
        },
        "domainpairs": {
            "table": "domain_pair_transformations_data",
            "joins": "inner join domain_pair_transformations on domain_pair_transformations_data.domain_pair_transformation_id = domain_pair_transformations.domain_pair_transformation_id inner join domain_pairs as before_domain_pairs on before_domain_pairs.domain_pair_id = domain_pair_transformations.before_domain_pair_id inner join domain_pairs as after_domain_pairs on after_domain_pairs.domain_pair_id = domain_pair_transformations.after_domain_pair_id inner join domains as before_domains1 on before_domains1.domain_id = before_domain_pairs.domain_id1 inner join domains as before_domains2 on before_domains2.domain_id = before_domain_pairs.domain_id2 inner join domains as after_domains1 on after_domains1.domain_id = after_domain_pairs.domain_id1 inner join domains as after_domains2 on after_domains2.domain_id = after_domain_pairs.domain_id2 inner join proteins as before_proteins on before_proteins.protein_id = before_domains1.protein_id inner join proteins as after_proteins on after_proteins.protein_id = after_domains1.protein_id inner join sequences on sequences.sequence_id = before_proteins.sequence_id"
        },
        "residues": {
            "table": "protein_labels",
            "joins": "inner join proteins on proteins.protein_id = protein_labels.protein_id inner join sequences on sequences.sequence_id = proteins.sequence_id"
        }
    },
